<section>
    <div class="container">
        <div class="row mt-5">
            {{#each products}}

            <div class="col-md-3 p-3">
                <div class="card" style="width:15erm; height:29rem">
                    <img class="card-img-top" src="/assets/images/{{this.product.Image}}" alt="Card image cap"
                        style="height:20rem">
                    <div class="card-body">
                        <h5 class="card-title">{{this.product.Name}}</h5>
                        <p class="card-text">RS.{{this.product.Price}}/-</p>
                        <p class="card-text">Quantity:{{this.quantity}}</p>
                        <p class="card-text">{{this.Discription}}</p>

                    </div>


                </div>
            </div>
            {{/each}}
        </div>
    </div>
</section>